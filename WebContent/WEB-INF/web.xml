<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>bibliosoft</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Servlet_NuevoSocio</servlet-name>
    <servlet-class>negocio.Servlet_NuevoSocio</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet_Login</servlet-name>
    <servlet-class>negocio.Servlet_Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet_AltaObra</servlet-name>
    <servlet-class>negocio.Servlet_AltaObra</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet_NuevoPrestamo</servlet-name>
    <servlet-class>negocio.Servlet_NuevoPrestamo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet_ModificaObra</servlet-name>
    <servlet-class>negocio.Servlet_ModificaObra</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Servlet_ModificaSocio</servlet-name>
    <servlet-class>negocio.Servlet_ModificaSocio</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet_NuevoSocio</servlet-name>
    <url-pattern>/registrar_usuario</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_AltaObra</servlet-name>
    <url-pattern>/alta_obra</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_NuevoPrestamo</servlet-name>
    <url-pattern>/nuevo_prestamo</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>/faces/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_ModificaObra</servlet-name>
    <url-pattern>/modifica_obra</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Servlet_ModificaSocio</servlet-name>
    <url-pattern>/modifica_socio</url-pattern>
  </servlet-mapping>
</web-app>